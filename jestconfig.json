{
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/out/"
  ],
  "collectCoverageFrom": [
    "src/jest_unit_testing/**/*.js",
    "!**/__tests__/**",
    "!**/node_modules/**"
  ],
  "moduleDirectories": [
    "node_modules",
    "<rootDir>/src"
  ],
  "testRegex": "(/__tests__/.*|\\-(test|spec)|\\.(test|spec))\\.(ts|tsx|js)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ]
}
